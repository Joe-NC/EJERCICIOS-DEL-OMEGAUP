#include <iostream>
#include <vector>
using namespace std;

int main() {
    int N;
    cin >> N;
    vector<int> digitCount(10, 0);

    for (int i = 0; i < N; i++) {
        int num;
        cin >> num;
        while (num > 0) {
            int digito = num % 10;
            digitCount[digito]++;
            num /= 10;
        }
    }
    for (int i = 0; i < 10; i++) {
        cout << "DÃ­gito " << i << ": " << digitCount[i] << " estampas" << endl;
    }

    return 0;
}