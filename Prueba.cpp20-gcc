#include <iostream>
using namespace std;

int main() {
    unsigned int num;
    while (true) {
        cin >> num;

        if (num == 0) {
            break;
        }
        unsigned long long suma = 0;

        for (unsigned int i = 0; i <= num; ++i) {
            suma += i;
        }
        if (suma % num == 0) {
            cout << "SI" << endl;
        } else {
            cout << "NO" << endl;
        }
    }
    return 0;
}