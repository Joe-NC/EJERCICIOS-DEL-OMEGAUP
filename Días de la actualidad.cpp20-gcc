#include <iostream>
#include <string>
using namespace std;

int main() {
    string linea;

    // Leer l√≠neas hasta encontrar el fin del archivo
    while (getline(cin, linea)) {
        for (char &caracter : linea)
            if (caracter == ' ') {
                caracter = '_';
            }
        }
        cout << linea << endl;
    }
    return 0;
}