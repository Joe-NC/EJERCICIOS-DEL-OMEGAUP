#include <iostream>
using namespace std;

int main() {

    int M;
    cin >> M;
    int naranjas = 0, limas = 0;
    for (int i = 0; i < M; ++i) {
        int E;
        cin >> E;
        if (E % 2 == 0) {
            naranjas++;
        } else {
            limas++;
        }
    }
    cout << naranjas << " " << limas << endl;
    return 0;
}